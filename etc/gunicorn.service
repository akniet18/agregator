[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/var/www/agregator/agregator
ExecStart=/var/www/agregator/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/var/www/agregator/agregator/gunicorn.sock \
          agregator.wsgi:application

[Install]
WantedBy=multi-user.target